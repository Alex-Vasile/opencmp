[DIRICHLET]
u = inlet  -> [y*(0.5-y)/0.5^2, 0.0]
    wall   -> [0.0, 0.0]
    circle -> [0.0, 0.0]
a = inlet -> 1.0
b = inlet -> 0.0

[STRESS]
stress = outlet -> [0.0, 0.0]

[TOTAL_FLUX]
a = outlet -> 0.0
    wall   -> 0.0
b = outlet -> 0.0
    wall   -> 0.0

[SURFACE_RXN]
a = circle -> -a
b = circle -> a
