[DIRICHLET]
u = wall    -> [0.0, 0.0]
    inlet   -> [1.5*4*y*(0.41-y)/(0.41*0.41), 0.0]
    cyl     -> [0.0, 0.0]
a = inlet   -> 1
b = inlet   -> 0

[NEUMANN]
a = wall    -> 0
    outlet  -> 0
b = wall    -> 0
    outlet  -> 0

[STRESS]
stress = outlet -> [0.0, 0.0]

[TOTAL_FLUX]

[SURFACE_RXN]
a = cyl -> 1
